{
  "name": "driving-between-cities",
  "version": "1.0.0",
  "description": "Driving time between Polish cities",
  "main": "js/main.js",
  "scripts": {
    "start": "nodemon js/main.js --exec babel-node --presets es2015,stage-2",
    "build": "babel js -d dist",
    "serve": "node dist/main.js",
    "lint": "eslint js/ --ext .js",
    "mocha": "mocha --compilers js:babel-core/register --require babel-polyfill ./test/mocha/",
    "ava": "ava",
    "test": "npm run mocha && npm run ava"
  },
  "ava": {
    "files": [
      "test/ava/*.js"
    ],
    "source": [
      "js/**/*.{js,jsx}"
    ],
    "concurrency": 4,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": "babel-register",
    "babel": "inherit"
  },
  "author": "≈Åukasz Sutkowski",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "mocha": "^3.1.1",
    "mockery": "^2.0.0",
    "nodemon": "^1.10.0",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "osrm": "^5.3.3",
    "pg-promise": "^5.3.3",
    "rxjs": "^5.0.0-rc.1"
  }
}
